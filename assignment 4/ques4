#include <iostream>
#include <queue>
using namespace std;

int main() {
    queue<char> q;
    int freq[26] = {0};
    string input;

    cout << "Enter characters (space separated): ";
    getline(cin, input);

    cout << "Output: ";

    for (char ch : input) {
        if (ch == ' ') continue;

        freq[ch - 'a']++;

        q.push(ch);

        while (!q.empty() && freq[q.front() - 'a'] > 1) {
            q.pop();
        }

        if (q.empty())
            cout << "-1 ";
        else
            cout << q.front() << " ";
    }

    cout << endl;
    return 0;
}
